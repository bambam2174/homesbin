#!/bin/zsh

list-array-elements ${GIT_EXCLUDES_FOLDERNAMES}
list-array-elements ${GIT_EXCLUDES_FOLDERPATHS}


echo ${GIT_EXCLUDES_FOLDERNAMES}
echo ${GIT_EXCLUDES_FOLDERPATHS}

echo ${#GIT_EXCLUDES_FOLDERNAMES}
echo ${#GIT_EXCLUDES_FOLDERPATHS}

echo ${UNICODE_RIGHT_DBLQUOTATION_MARK}${USER}${UNICODE_RIGHT_DBLQUOTATION_MARK}

echo ${UNICODE_RIGHT_DBLQUOTATION_MARK}${ARR_FOO}${UNICODE_RIGHT_DBLQUOTATION_MARK}
echo ${UNICODE_RIGHT_DBLQUOTATION_MARK}${#ARR_FOO}${UNICODE_RIGHT_DBLQUOTATION_MARK}
echo ${UNICODE_RIGHT_DBLQUOTATION_MARK}${ARR_FOO[1]}${UNICODE_RIGHT_DBLQUOTATION_MARK}
echo ${UNICODE_RIGHT_DBLQUOTATION_MARK}${ARR_FOO[-1]}${UNICODE_RIGHT_DBLQUOTATION_MARK}

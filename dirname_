#!/bin/zsh

IFS=${DELIMITER_ENDL}

isDebug && echo ${@}

if [[ ${#} -gt 0 ]]
then
    local CNT=0
    for TMP_PATH in "${@}"
    do
        isDebug && echo "Pfad Nr.$((CNT+=1)) ${TMP_PATH}"
        ARR_PATH=("${TMP_PATH}")
    done
else
    INPUT_PIPE="$(cat < /dev/stdin)"
    [[ -n ${INPUT_PIPE} ]] && ARR_PATH=($(echo ${INPUT_PIPE}))
fi

for currPath in ${ARR_PATH}
do
    dirname ${currPath}
done



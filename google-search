#!/bin/zsh

QRY=( steven crowder demon demonic 666 )

BROWSER_IT=true
CALL_BY=browser

startsWithDash ${1} && ([[ ${1/-/} = "c" ]] || ${1/-/} = "-curl" ) && CURL_IT=true && CALL_BY=true

#startsWithDash ${1} && ([[ ${1/-/} = "b" ]] || ${1/-/} = "-browser" ) && BROWSER_IT=true


URL_QRY='https://www.google.com/search?q='$(arrjoin -c "+" ${@:-$QRY})'&oq='$(arrjoin -c "+" ${@:-$QRY})'&aqs=chrome..69i64j69i57.5638j0j9&sourceid=chrome&ie=UTF-8'


#!/bin/zsh
echo $0

CMDOUTPUT1="$1"
CMDOUTPUT2="$2"

echo $CMDOUTPUT1
echo $CMDOUTPUT2

$(echo $CMDOUTPUT1) > $TMP/CMDOUTPUT1.TMP.TXT
$(echo $CMDOUTPUT2) > $TMP/CMDOUTPUT2.TMP.TXT


tail -n +1  $TMP/CMDOUTPUT*.TMP.TXT

diff $TMP/CMDOUTPUT1.TMP.TXT $TMP/CMDOUTPUT2.TMP.TXT

TMP_FILES="ls -1  $TMP/CMDOUTPUT*.TMP.TXT"
echo $TMP_FILES
$(echo $TMP_FILES)
DELETE_TMP_FILES='rm -f $TMP/CMDOUTPUT*.TMP.TXT'

echo "ToDo"
echo $DELETE_TMP_FILES

_get_tmp_target() {
    echo '$TMP/CMDOUTPUT'$1'.TMP.TXT'
}


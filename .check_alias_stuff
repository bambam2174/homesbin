#!/bin/zsh

export CHECKOCCURENCES=CHECKOCCURENCES

add_ls() {
    echo $0
     [ -z "$ARR_ALIAS" ] && export ARR_ALIAS=("start");
     ARR_ALIAS+="$1"
}

showshit () {
    for i in $ARR_ALIAS
    do
        echo $i
    done
}


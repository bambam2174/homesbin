#!/bin/zsh

IFS=${DELIMITER_ENDL}

SCRIPTNAME="${0##*/}"

if [[ ${SHLVL} -gt 1 ]] 
then 
    echo "$(ecko ' add ' 118 88)" > /dev/stdin
    ecko "alias ${SCRIPTNAME}=\"source ${SCRIPTNAME}\"" 118 88 #> /dev/stdout
    #ecko " execute " 118 88 > /dev/stdin 2> /dev/null
    echo "$(ecko ' execute ' 118 88)" > /dev/stdin
#    ecko "\$(echo ${SCRIPTNAME}\"" 121 88
    exit 0
fi

_create-foo () {
	SZ_FUNC="_foo() { ${1} }" 
	eval "${SZ_FUNC}"
}

_alias-root3() {
    _create-foo "${1}"
    IFS=${DELIMITER_ENDL}
    ARR_WW=(`ww _foo`)
    list-array-elements $ARR_WW[2,-2]
}

_alias-root2 () {
    _create-foo "${1}"
	which _foo | head -n 2 | tail -n 1
}
_alias-root3 ${@}

#!/bin/zsh
OPTS=()
TMPINPUT="${1}"

[[ ${#} -gt 0 ]] && startsWithDash ${TMPINPUT} && [[ ${TMPINPUT[2]} = 'c' ]] && echo ${#SUPER_DIGITS} && . rts 0
[[ ${#} -gt 0 ]] && startsWithDash ${TMPINPUT} && [[ ${TMPINPUT[2]} =~ '[^0-9]' ]] && OPTS+="${1}" && shift

_char-digit-super () {
	[[ ${#} -eq 0 ]] && { echo ${OPTS} ${SUPER_DIGITS}; . rts 0 }
    for IDX in ${@}
    do
	    isNumeric ${IDX} && [[ ${IDX} -le ${#SUPER_DIGITS} ]] && echo ${OPTS} ${SUPER_DIGITS[${IDX}]} || echo ${OPTS} ${IDX}
    done
}
_char-digit-super ${@}
